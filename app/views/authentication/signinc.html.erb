
 <%# <div class="container">
    <div class="jumbotron">
      <h1>welcome to enginectra</h1>
      <h2>sign in as a customer</h2>
<form>
  <div class="form-group">
    <label for="exampleformcontrolinput1">email address</label>
    <input type="email" class="form-control" id="custemail" placeholder="name@example.com">
  </div>

  <div class="form-group">
    <label for="exampleformcontroltextarea1">password</label>
    <textarea class="form-control" id="custpassword" rows="1"></textarea>
  </div>
   <button type="submit" class="btn btn-primary" href = "inspectionform">submit</button>
</form>
</div>
</div>   %>

<p>Sign In</p>
<%= form_for @customer, :as => :customer, :url => signinc_path(@customer) do |f| %>
  <p>
    <%= f.label 'email:' %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label 'password:' %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Sign In' %>&nbsp;&nbsp;&nbsp;
    <%= f.submit 'Clear Form', :type => 'reset' %>
  </p>
<% end %>


